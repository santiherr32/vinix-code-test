{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "*": [
        "node_modules/*",
      ],
    },
    "jsx": "react-jsx",
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "src",
    "*.js"
  ]
}